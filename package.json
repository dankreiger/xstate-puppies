    "@testing-library/user-event": "7.2.1",
    "@types/jest": "24.9.1",
    "@types/node": "12.12.26",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@xstate/react": "0.8.1",
    "husky": "4.2.1",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.1",
    "sort-package-json": "1.39.1",
